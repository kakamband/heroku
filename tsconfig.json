{
  "compilerOptions": {
    "outDir": "lib",
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "declaration": false,
    "pretty": true,
    "strictNullChecks": true,
    "lib": ["es2015", "esnext.asynciterable"]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
